{
  "name": "yourvendor/pro",
  "description": "A simple PHP REST API project using MVC structure.",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Controllers\\": "src/Controllers/",
      "Services\\": "src/Services/",
      "Helpers\\": "src/Helpers/",
      "Config\\": "src/Config/",
      "Models\\": "src/Models/",
      "Middlewares\\": "src/Middlewares/",
      "Src\\": "src/"
    }
